<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mvvmarchitecture">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".base.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.MVVMArchitecture">
        <activity
            android:name=".multipart_image.UploadImage"
            android:exported="true"
            android:label="@string/title_activity_upload_image"
            android:theme="@style/Theme.MVVMArchitecture">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".multilevel.DropdownAndSortActivity"
            android:exported="true"
            android:label="@string/title_activity_dropdown_and_sort"
            android:theme="@style/Theme.MVVMArchitecture">
        </activity>
        <activity
            android:name=".list.ui.ApiListActivity"
            android:exported="true"
            android:label="@string/title_activity_api_list"
            android:theme="@style/Theme.MVVMArchitecture.NoActionBar"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MVVMArchitecture.NoActionBar" />
    </application>

</manifest>